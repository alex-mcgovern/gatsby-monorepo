{
  "git.ignoreLimitWarning": true,
  "cSpell.words": [
    "algoliasearch",
    "bloks",
    "Bulbasaur",
    "Bulbizarre",
    "capitalise",

    "deduped",
    "deduplicated",

    "frontmatter",
    "gtag",
    "hubspot",
    "hypergrowth",

    "instantsearch",
    "jank",
    "janky",
    "localised",
    "logomark",
    "neutral_text_lowContrast",
    "pokedex",
    "sanitised",
    "specialised",
    "startpage",
    "stylelint",
    "testid",
    "truelayer"
  ],
  "conventionalCommits.scopes": ["CodebaseStructure"],
  "stylelint.configFile": ".stylelintrc.json",
  "commentBox.styles": {
    "h1": {
      "leftEdgeToken": "//      ",
      "rightEdgeToken": "",
      "topEdgeToken": "—",
      "bottomEdgeToken": "—",

      "commentStartToken": "/* ",
      "topRightToken": "—",
      "bottomLeftToken": "// ",
      "commentEndToken": "— */",

      "textAlignment": "left",
      "boxWidth": 80
    },
    "h2": {
      "leftEdgeToken": "//      ",
      "rightEdgeToken": "",
      "topEdgeToken": "—",
      "bottomEdgeToken": "—",

      "commentStartToken": "/* ",
      "topRightToken": "—",
      "bottomLeftToken": "// ",
      "commentEndToken": "— */",

      "textAlignment": "left",
      "boxWidth": 48
    },
    "h1_JSX": {
      "leftEdgeToken": "//      ",
      "rightEdgeToken": "",
      "topEdgeToken": "—",
      "bottomEdgeToken": "—",

      "commentStartToken": "{/* ",
      "topRightToken": "—",
      "bottomLeftToken": "// ",
      "commentEndToken": "— */}",

      "textAlignment": "left",
      "boxWidth": 80
    },
    "h2_JSX": {
      "leftEdgeToken": "//      ",
      "rightEdgeToken": "",
      "topEdgeToken": "—",
      "bottomEdgeToken": "—",

      "commentStartToken": "{/* ",
      "topRightToken": "—",
      "bottomLeftToken": "// ",
      "commentEndToken": "— */}",

      "textAlignment": "left",
      "boxWidth": 48
    }
  }
}
