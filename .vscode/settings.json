{
  "search.exclude": {
    "**/package-lock.json": true,
    "**/node_modules": true,
    "**/.cache": true,
    "**/*.snap": true
  },
  "commentBox.styles": {
    "h1": {
      "bottomEdgeToken": "—",
      "bottomLeftToken": "* ",
      "boxWidth": 80,
      "commentEndToken": "— */",
      "commentStartToken": "/** ",
      "leftEdgeToken": "* ",
      "rightEdgeToken": "",
      "textAlignment": "left",
      "topEdgeToken": "—",
      "topRightToken": "—"
    },
    "h1_JSX": {
      "bottomEdgeToken": "—",
      "bottomLeftToken": "* ",
      "boxWidth": 80,
      "commentEndToken": "— */}",
      "commentStartToken": "{/** ",
      "leftEdgeToken": "* ",
      "rightEdgeToken": "",
      "textAlignment": "left",
      "topEdgeToken": "—",
      "topRightToken": "—"
    },
    "h2": {
      "bottomEdgeToken": "—",
      "bottomLeftToken": "* ",
      "boxWidth": 48,
      "commentEndToken": "— */",
      "commentStartToken": "/** ",
      "leftEdgeToken": "* ",
      "rightEdgeToken": "",
      "textAlignment": "left",
      "topEdgeToken": "—",
      "topRightToken": "—"
    },
    "h2_JSX": {
      "bottomEdgeToken": "—",
      "bottomLeftToken": "* ",
      "boxWidth": 48,
      "commentEndToken": "— */}",
      "commentStartToken": "{/** ",
      "leftEdgeToken": "* ",
      "rightEdgeToken": "",
      "textAlignment": "left",
      "topEdgeToken": "—",
      "topRightToken": "—"
