  ],
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-mcgovern/alexmcgovern.com.git"
  },
  "scripts": {
    "bootstrap": "lerna clean --yes && lerna bootstrap --use-workspaces",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "firebase:emulators": "firebase emulators:start",
    "firebase:emulators:exec": "firebase emulators:exec",
    "jest": "jest .",
    "jest:watch": "jest --watch",
    "lint": "eslint . --fix",
    "boondoggle:jest": "jest --select-projects boondoggle.design",
    "demo-feedback:cy:open": "start-server-and-test demo-feedback:develop http://localhost:8000 cy:open",
    "demo-feedback:cy:open:emulate-firebase": "start-server-and-test demo-feedback:develop:emulate-firebase http://localhost:8000 cy:open",
    "demo-feedback:cy:run": "start-server-and-test demo-feedback:develop http://localhost:8000 cy:run --spec 'cypress/e2e/packages/demo-feedback-form/**/*'",
    "demo-feedback:cy:run:emulate-firebase": "npm run firebase:emulators:exec 'npm run demo-feedback:cy:run'",
    "demo-feedback:develop": "cd packages/demo-feedback-form && gatsby develop",
    "demo-feedback:develop:emulate-firebase": "cd packages/demo-feedback-form && GATSBY_USE_FIREBASE_EMULATOR=true gatsby develop",
    "demo-feedback:jest": "jest --select-projects demo-feedback-form",
    "demo-pokedex:cy": "start-server-and-test pokedex:develop http://localhost:8000 cy:open",
    "demo-pokedex:develop": "cd packages/demo-pokedex && gatsby develop",
    "demo-pokedex:jest": "jest --select-projects gatsby-pokedex",
    "gatsby-site:cy:open": "start-server-and-test gatsby-site:develop http://localhost:8000 cy:open",
    "gatsby-site:develop": "cd packages/gatsby-site && gatsby develop",
    "gatsby-site:jest": "jest --select-projects gatsby-site"
  },
  "workspaces": [
    "packages/*"
  ]
}
